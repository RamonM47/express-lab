<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Express App Review</title>
</head>

<body>
    <h1>Student Attendance</h1>

    <body>
        <h1>students</h1>
        <% if(error) { %>
            <h2>
                <%= error %>
            </h2>
            <% } %>
                <ul>
                    <% students.forEach(function(student) { %>
                        <li>
                            <%= student.text %>
                                -
                                <%= student.done ? 'Here' : 'Not here' %>
                        </li>
                        <% }) %>
                </ul>
    </body>
</body>

</html>